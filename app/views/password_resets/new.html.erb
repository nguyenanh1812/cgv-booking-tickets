<title> Forgot password</title>
<h1>Forgot password</h1>
<div class="cgv-login-register-form">
    <div class="r-login">
        <%= form_for(:password_reset, url: password_resets_path) do |f| %>
            <%= f.label :email %>
            <%= f.email_field :email, class: 'form-control' %>
            <%= f.submit "Submit" %>
        <% end %>
    </div>
</div>